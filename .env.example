# .env.example
"""
Example environment file for FloatBot configuration
Copy to .env and fill in your values
"""
# Database configuration
DB_HOST=your-neon-host.neon.tech
DB_PORT=5432
DB_NAME=neondb
DB_USER=neondb_owner
DB_PASSWORD=your_password

# GDAC configuration
GDAC_HOST=https

# Processing configuration  
BATCH_SIZE=1000
MAX_RETRIES=3

# Logging configuration
LOG_LEVEL=INFO
LOG_FILE=floatbot.log